/* ROUNDED THEME FOR ROFI */
/* Author: Newman Sanchez (https://github.com/newmanls) */

* {
    bg0:    #2E3440F2;
    bg1:    #3B4252;
    bg2:    #4C566A80;
    bg3:    #394050F2;  
    fg0:    #D8DEE9;
    fg1:    #ECEFF4;
    fg2:    #D8DEE9;
    fg3:    #4C566A;
}

@import "./template.rasi"

element selected {
    text-color: @bg1;
}

configuration {
	// Keyboard bindings.
  kb-remove-to-eol: "";
  kb-accept-entry: "Return";
	kb-row-up: "Control+k";
	kb-row-down: "Control+j";
}
